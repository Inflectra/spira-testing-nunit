<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:mstns="http://www.inflectra.com/SpiraTest/DataSets/TestRunDataSet.xsd" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata" xmlns="http://www.inflectra.com/SpiraTest/DataSets/TestRunDataSet.xsd" attributeFormDefault="qualified" elementFormDefault="qualified" targetNamespace="http://www.inflectra.com/SpiraTest/DataSets/TestRunDataSet.xsd" id="TestRunData" xmlns:xs="http://www.w3.org/2001/XMLSchema">
